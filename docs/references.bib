@article{shah_cell-intrinsic_2022,
	title = {Cell-intrinsic Aryl Hydrocarbon Receptor signalling is required for the resolution of injury-induced colonic stem cells},
	volume = {13},
	issn = {2041-1723},
	url = {https://www.nature.com/articles/s41467-022-29098-7},
	doi = {10.1038/s41467-022-29098-7},
	abstract = {Abstract
            
              The aryl hydrocarbon receptor ({AHR}) is an environmental sensor that integrates microbial and dietary cues to influence physiological processes within the intestinal microenvironment, protecting against colitis and colitis-associated colorectal cancer development. Rapid tissue regeneration upon injury is important for the reinstatement of barrier integrity and its dysregulation promotes malignant transformation. Here we show that {AHR} is important for the termination of the regenerative response and the reacquisition of mature epithelial cell identity post injury in vivo and in organoid cultures in vitro. Using an integrative multi-omics approach in colon organoids, we show that {AHR} is required for timely termination of the regenerative response through direct regulation of transcription factors involved in epithelial cell differentiation as well as restriction of chromatin accessibility to regeneration-associated
              Yap/Tead
              transcriptional targets. Safeguarding a regulated regenerative response places {AHR} at a pivotal position in the delicate balance between controlled regeneration and malignant transformation.},
	pages = {1827},
	number = {1},
	journaltitle = {Nature Communications},
	shortjournal = {Nat Commun},
	author = {Shah, Kathleen and Maradana, Muralidhara Rao and Joaquina Delàs, M. and Metidji, Amina and Graelmann, Frederike and Llorian, Miriam and Chakravarty, Probir and Li, Ying and Tolaini, Mauro and Shapiro, Michael and Kelly, Gavin and Cheshire, Chris and Bhurta, Deendyal and Bharate, Sandip B. and Stockinger, Brigitta},
	urldate = {2025-10-28},
	date = {2022-04-05},
	langid = {english},
	annotation = {nf-core/atacseq pipeline. application
},
	file = {Full Text:C\:\\Users\\migue\\Zotero\\storage\\GVIDHZQT\\Shah et al. - 2022 - Cell-intrinsic Aryl Hydrocarbon Receptor signalling is required for the resolution of injury-induced.pdf:application/pdf},
