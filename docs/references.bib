
@article{love_moderated_2014,
	title = {Moderated estimation of fold change and dispersion for {RNA}-seq data with {DESeq}2},
	volume = {15},
	issn = {1474-760X},
	doi = {10.1186/s13059-014-0550-8},
	abstract = {In comparative high-throughput sequencing assays, a fundamental task is the analysis of count data, such as read counts per gene in {RNA}-seq, for evidence of systematic changes across experimental conditions. Small replicate numbers, discreteness, large dynamic range and the presence of outliers require a suitable statistical approach. We present {DESeq}2, a method for differential analysis of count data, using shrinkage estimation for dispersions and fold changes to improve stability and interpretability of estimates. This enables a more quantitative analysis focused on the strength rather than the mere presence of differential expression. The {DESeq}2 package is available at http://www.bioconductor.org/packages/release/bioc/html/{DESeq}2.html webcite.},
	pages = {550},
	number = {12},
	journaltitle = {Genome Biology},
	shortjournal = {Genome Biol},
	author = {Love, Michael I. and Huber, Wolfgang and Anders, Simon},
	date = {2014},
	pmid = {25516281},
	pmcid = {PMC4302049},
	keywords = {{RNA}, Software, Algorithms, High-Throughput Nucleotide Sequencing, Sequence Analysis, {RNA}, Computational Biology, Models, Genetic},
	annotation = {{DEA} Differential Expression Analysis with {DEseq}2
},
	file = {Full Text:C\:\\Users\\migue\\Zotero\\storage\\SQ4REGRL\\Love et al. - 2014 - Moderated estimation of fold change and dispersion.pdf:application/pdf},
}

@article{motallebnejad_integrated_2025,
	title = {Integrated {\textless}span style="font-variant:small-caps;"{\textgreater}{ATAC}{\textless}/span{\textgreater} ‐seq and {\textless}span style="font-variant:small-caps;"{\textgreater}{RNA}{\textless}/span{\textgreater} ‐seq analysis identifies key regulatory elements in {\textless}span style="font-variant:small-caps;"{\textgreater}{NK}{\textless}/span{\textgreater} cells activated with feeder cells and {\textless}span style="font-variant:small-caps;"{\textgreater}{IL}{\textless}/span{\textgreater} ‐2},
	volume = {10},
	issn = {2380-6761, 2380-6761},
	url = {https://aiche.onlinelibrary.wiley.com/doi/10.1002/btm2.10747},
	doi = {10.1002/btm2.10747},
	shorttitle = {Integrated {\textless}span style="font-variant},
	abstract = {Abstract
            Natural killer ({NK}) cells are in development for allogeneic immunotherapy; however, for such use as off‐the‐shelf medicines, {NK} cells need to undergo ex vivo expansion, typically through activation with feeder cells and cytokines, to generate sufficient cells for clinical applications. Upon stimulation with feeder cells in the presence of cytokines, {NK} cells undergo profound changes in gene expression, altering their metabolic activity, cell cycle progression, and growth behavior, but the precise changes that drive this transformation remain poorly understood. In this study, we identified significant differences in the transcriptome and chromatin accessibility of {NK} cells 7 days after feeder cell and cytokine activation, with the changes even more pronounced in genome regions closer to enhancers. Several transcription factors, including {AP}‐1, {IRF}4, {STATs}, T‐bet, Eomes, and {bHLHE}40, which play key roles in {NK} cell development and immune response, exhibited differential binding activity between unstimulated and day 7 {NK} cells. Gene sets composed of target genes downstream of these transcription factors were also enriched at day 7, implying their involvement in {NK} cell activation. Moreover, we compared potential super‐enhancer regions in {NK} cells before and after activation, combined with the transcriptional activity of nearby genes. We identified stable and transcriptionally active super‐enhancers in unstimulated and day 7 {NK} cells, as well as those that form or disappear after co‐culture initiation. The transcriptomic and epigenetic characterization of {NK} cells presented in this study could facilitate the ex vivo expansion and engineering of functionally superior {NK} cells.},
	pages = {e10747},
	number = {3},
	journaltitle = {Bioengineering \& Translational Medicine},
	shortjournal = {Bioengineering \& Transla Med},
	author = {Motallebnejad, Pedram and Lee, Zion and One, Jennifer L. and Cichocki, Frank and Hu, Wei‐Shou and Azarin, Samira M.},
	urldate = {2025-10-28},
	date = {2025-05},
	langid = {english},
	annotation = {Multiomics Integrations {ATAC}-seq X {RNA}-seq
},
	file = {Full Text:C\:\\Users\\migue\\Zotero\\storage\\Q5DKIXPB\\Motallebnejad et al. - 2025 - Integrated .pdf:application/pdf},
}

